&lt;style type=&quot;text/css&quot;&gt;
&lt;!--
    .moving {
        position: absolute;
        visibility: show;
        left: 0px;
        top: 0px;
        width: 1px;
        height: 1px;
    }
--&gt;
&lt;/style&gt;

&lt;div id=&quot;hatbaby&quot; class=&quot;moving&quot; visibility=&quot;hide&quot;&gt;&lt;img id=&quot;skull&quot; src=&quot;http://divinewrath.com/image/skull-01.gif&quot;&gt;&lt;/img&gt;&lt;/div&gt;

&lt;script language=&quot;JavaScript&quot;&gt;
&lt;!--
    //  Okay, JS doesn't support constants, but we can put 'em in all 
    //  caps and PRETEND they're constants, okay? OKAY?!
    var TIMER_DELAY = 20;
    var GRAVITY = 0.5;
    var anim_timer = -1;
    var skull = obj_ref( document, 'hatbaby' );

    var DEFAULT_VSPEED = 5;
    skull.vspeed = DEFAULT_VSPEED;

    function on_load() {
//        var skull = obj_ref( document, 'hatbaby' );
        set_vis( skull, false );
        start_anim();
    }

    function start_anim() {
        if ( skull == null ) {
            return;
//            alert( 'skull == null\n\nskull skull skull, skull is the null, sk-sk-sk-skull skull...' );
        }
        if ( anim_timer == -1 || true ) {
            //  80 is a vague guess. Not a big deal.
            set_pos( skull, ( window_width() - 80 ) * Math.random(), 0 );
            set_vis( skull, true );
            anim_timer = window.setInterval( &quot;on_anim_timer()&quot;, TIMER_DELAY );
        }
    }

    function stop_anim() {
        if ( skull == null ) {
            return;
//            alert( 'skull == null\n\nskull skull skull, skull is the null, sk-sk-sk-skull skull...' );
        }
        if ( anim_timer != -1 ) {
            window.clearInterval( anim_timer );
            anim_timer = -1;
            //  Fire off another skull in ten to twenty seconds
            window.setTimeout( 'start_anim()', 10000 + Math.random() * 10000 );
        }
    }

    //  This is where stuff happens.
    function on_anim_timer() {
        if ( skull == null )
            return;

        set_vis( skull, true );

        var left    = get_x( skull );
        var top     = get_y( skull );
        var cx      = get_width( skull )
        var cy      = get_height( skull )

        var newtop = top + skull.vspeed;

        skull.vspeed += GRAVITY;

        if ( newtop + cy &gt;= window_height() ) {
            //  If it's down at the bottom of the window, hide it and start over
            //  at some random (ha ha) point at the top of the window.

            //  Fucking IE 5.5 never shows it again if I hide it. I haven't 
            //  had that problem w/ other things w/ that version. WTF?
            if ( IS_NS )
                set_vis( skull, false );
            else
                set_pos( skull, -200, 0 );
            skull.vspeed = DEFAULT_VSPEED;
//            set_pos( skull, ( window_width() - cx ) * Math.random(), 0 );
            stop_anim();
        } else {
            set_pos( skull, left, newtop );
        }
    }
//--&gt;
&lt;/script&gt;
